<% content_for :titlebar do %>
  <h1><%= link_to company.name, company_admin_dashboard_url %>
    > Project Types</h1>
  <div class="nav">
    <%= link_to "New Project Type", new_company_admin_project_type_url %>
  </div>
<% end %>

<% content_for :sidebar do %>
  <%= render :partial => 'company_admin/shared/sidebar' %>
<% end %>


<table class="grid">
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  <% project_types.each do |project_type| %>
    <tr>
      <td><%= project_type.name %></td>
      <td><%= link_to 'Edit', company_admin_project_type_url(project_type) %></td>
    </tr>
  <% end %>
</table>

<div class="digg_pagination">
  <%= will_paginate project_types %>
</div>



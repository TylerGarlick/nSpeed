<%= form_for [:company_admin, role] do |f| %>
    <div class="fields">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <%= intelligent_table("users", company_admin_users_url(:json), [:username], :username, role.users) %>



    <div class="buttons">
      <%= f.submit role.new_record? ? "Create" : "Update" %> <%= link_to "Cancel", company_admin_roles_url %>
    </div>
<% end  %>

